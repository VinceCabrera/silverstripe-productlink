{
	"name": "vincecabrera/silverstripe-productlink",
	"description": "Adds a product option to TinyMCE's Insert Link dropdown for creating/editing tel: links using native SilverStripe 4 modals",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/VinceCabrera/silverstripe-productlink",
	"keywords": [
		"silverstripe",
		"cms",
		"tinymce"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Vince Cabrera",
			"email": "vince.cabrera@riobravo.co.nz"
		},
		{
			"name": "Rio Bravo",
			"homepage": "https://riobravo.nz",
			"email": "vince.cabrera@riobravo.co.nz"
		}
	],
	"support": {
		"issues": "https://github.com/vincecabrera/silverstripe-productlink/issues"
	},
	"require": {
		"silverstripe/cms": "^4.4"
	},
	"autoload": {
		"psr-4": {
			"VinceCabrera\\ProductLink\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist"
		]
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
