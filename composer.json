{
	"name": "firebrand/silverstripe-phonelink",
	"description": "Adds a telephone number option to TinyMCE's Insert Link dialog for creating/editing tel: links using native SilverStripe 4 modals",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/firebrand/silverstripe-phonelink",
	"keywords": [
		"silverstripe",
		"cms",
		"tinymce"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Firebrand Developers",
			"email": "developers@firebrand.nz"
		}
	],
	"support": {
		"issues": "https://github.com/firebrand/silverstripe-phonelink/issues"
	},
	"require": {
		"silverstripe/cms": "^4.1.2"
	},
	"autoload": {
		"psr-4": {
			"Firebrand\\PhoneLink\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist",
			"client/lang"
		]
	}
}
